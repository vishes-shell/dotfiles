{
  "date": {
    "prefix": "date",
    "body": ["${CURRENT_DATE}.${CURRENT_MONTH}.${CURRENT_YEAR}"],
    "description": "Put the date in (d.m.Y) format"
  },
  "time": {
    "prefix": "time",
    "body": ["${CURRENT_HOUR}:${CURRENT_MINUTE}"],
    "description": "I give you back the time (H:M)"
  },
  "timeHMS": {
    "prefix": "timeHMS",
    "body": ["${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}"],
    "description": "I give you back the time (H:M:S)"
  },
  "datetime": {
    "prefix": "datetime",
    "body": ["${CURRENT_DATE}.${CURRENT_MONTH}.${CURRENT_YEAR} ${CURRENT_HOUR}:${CURRENT_MINUTE}"],
    "description": "I give you back the time and date (d.m.Y H:M)"
  },
}
