  "jiracapturekey": {
    "prefix": [
      "jiracapturekey",
      "jirasavekey",
      "jiraissueenv"
    ],
    "body": [
      "| grep -o 'LMSDEV-[[:digit:]]*' | tee /tmp/capture.out && set ISSUE $(cat /tmp/capture.out)",
      "$0"
    ],
    "description": "Insert cap command to capture issue key from jira-cli"
  },
  "jirabackprocess": {
